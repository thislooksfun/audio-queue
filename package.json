{
  "name": "audio-queue",
  "version": "1.0.0",
  "description": "A universal audio queue",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "build": "npm run clean && tsc && (cd frontend && npm run build)",
    "clean": "rm -rf build",
    "dev": "npm run build && npm start",
    "start": "node ./build/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "prettier:check": "prettier --check '**/*'",
    "prettier:fix": "prettier --write '**/*'",
    "purge": "rm -rf node_modules ; rm -rf data ; rm -rf build ; (cd frontend && npm run purge)",
    "postinstall": "(cd frontend && npm i)"
  },
